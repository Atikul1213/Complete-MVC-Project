<h2>WareHouse Create page</h2>

@model WareHouseModel

<form method="post">
    <div class="border p-3 mt-4">
        <div class="row pb-2">
            <h2 class="text-primary"> Create WareHouse</h2>
            <hr />
        </div>

        <div class="mb-3 row p-1">
            <label asp-for="Name" class="p-0"> </label>
            <input asp-for="Name" class="form-control" />
            <span asp-validation-for="Name" class="text-danger"></span>
        </div>

        <div class="row">
            <div class="col-6 col-md-3">
                <button class="btn-outline-primary">Create</button>
            </div>

            <div class="col-6 col-md-3">
                <a asp-controller="WareHouse" asp-action="Index" class="btn btn-outline-secondary">Back to List</a>
            </div>

        </div>


    </div>


</form>

@section Scripts {
    @{
        <partial name="_ValidationScriptsPartial"></partial>
    }

}